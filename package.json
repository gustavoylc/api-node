{
  "name": "api-node",
  "version": "1.0.0",
  "description": "API Node example",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "start": "node",
    "make:migration": "sequelize-cli migration:generate --name",
    "make:seed": "sequelize-cli db:seed",
    "make:model": "sequelize-cli model:generate --name",
    "make:seeder": "sequelize-cli seed:generate --name",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "migrate:revert": "sequelize-cli db:migrate:undo",
    "seed:revert": "sequelize-cli db:seed:undo",
    "migrate:reset": "sequelize-cli db:migrate:undo:all",
    "seed:reset": "sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "Gustavo Leyte-Vidal <glcdmx@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "nodemon": "2.0.20",
    "prettier": "2.8.1"
  },
  "dependencies": {
    "@hapi/boom": "10.0.0",
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "joi": "17.7.0",
    "pg": "8.8.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.27.0",
    "sequelize-cli": "6.5.2"
  }
}
